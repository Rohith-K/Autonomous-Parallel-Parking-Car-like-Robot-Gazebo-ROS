    <link name="wheel_rl">
      <inertial>
        <origin xyz="0 0 -0.021117" rpy="0 3.141593 0"/>
        <mass value="10.0"/>
        <inertia ixx="0.305583" ixy="0" ixz="0" iyy="0.305583" iyz="0" izz="0.5445"/>

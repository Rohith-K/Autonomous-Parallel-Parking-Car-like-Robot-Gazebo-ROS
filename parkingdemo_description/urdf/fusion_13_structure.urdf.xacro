<?xml version="1.0"?>
<!-- created with Phobos 0.7 -->
  <robot name="parkingdemo" xmlns:xacro="http://www.ros.org/wiki/xacro">
    <xacro:arg name="dae_file" default="$(find parkingdemo_description)/meshes/fusion_13/body.dae"/>
    
    <link name="base_footprint">
    </link>

    <link name="base_link">
      <inertial>
        <origin xyz="1.4225 0 0.25" rpy="0 0 0"/>
        <mass value="1500.0"/>
        <inertia ixx="1000.0" ixy="0" ixz="0" iyy="1000.0" iyz="0" izz="1000.0"/>
      </inertial>
      <visual name="chassis">
        <origin xyz="1.3641 0 -0.373676" rpy="0 0 1.570796"/>
        <geometry>
          <mesh filename="file://$(arg dae_file)" scale="1 0.97 1"/>
        </geometry>
        <material name="black.001"/>
      </visual>
      <collision name="chassis_collision">
        <origin xyz="1.51 -0.03046 0.325149" rpy="1.570796 0 -3.141593"/>
        <geometry>
          <mesh filename="package://parkingdemo_description/meshes/body_collision.stl" scale="0.000965 0.001 0.001"/>
        </geometry>
      </collision>
    </link>

    <link name="steer_fl">
      <inertial>
        <origin xyz="-0.013054 -0.0295 0" rpy="0 0 0"/>
        <mass value="1.0"/>
        <inertia ixx="0.037227" ixy="0" ixz="0" iyy="0.04" iyz="0" izz="0.06"/>
      </inertial>
    </link>

    <link name="steer_fr">
      <inertial>
        <origin xyz="-0.01305 0.0275 0" rpy="0 0 0"/>
        <mass value="1.0"/>
        <inertia ixx="0.037227" ixy="0" ixz="0" iyy="0.04" iyz="0" izz="0.06"/>
      </inertial>
    </link>

    <link name="wheel_fl">
